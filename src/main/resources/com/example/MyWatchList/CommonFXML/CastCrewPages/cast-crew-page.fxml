<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MyWatchList.Controllers.CommonComponent.CastCrewPages.CastCrewPageController">
   <content>
      <HBox alignment="CENTER" spacing="150.0" styleClass="hbox">
         <children>
            <VBox>
               <children>
                  <Label text="Cast">
                     <font>
                        <Font name="Arimo Regular" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <VBox fx:id="castVbox" alignment="TOP_CENTER" spacing="10.0" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Crew">
                     <font>
                        <Font name="Arimo Regular" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <VBox fx:id="crewVbox" alignment="TOP_CENTER" spacing="10.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </content>
</ScrollPane>
