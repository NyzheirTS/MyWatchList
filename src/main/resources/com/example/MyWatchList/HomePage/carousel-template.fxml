<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="carouselItem" alignment="CENTER" prefWidth="1665.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MyWatchList.Controllers.HomePage.CarouselController">
   <children>
      <Button fx:id="backward" mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="B" visible="false">
         <font>
            <Font size="22.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <HBox fx:id="posterContainer" alignment="CENTER" prefHeight="313.0" prefWidth="992.0" spacing="25.0" style="-fx-background-color: grey;">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
      <Button fx:id="forward" mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="F">
         <font>
            <Font size="22.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
   </children>
</HBox>
